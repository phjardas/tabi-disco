{
  "name": "tabidisco-server",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "TABIDISCO_MOCK_PI=true nodemon --watch src --ext ts --exec npx ts-node src/index.ts",
    "build": "tsc",
    "test": "jest"
  },
  "engines": {
    "node": "^8"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "helmet": "^3.10.0",
    "inversify": "^4.10.0",
    "lame": "^1.2.4",
    "mfrc522-rpi": "^1.1.0",
    "mp3-parser": "^0.3.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^5.5.6",
    "socket.io": "^2.0.4",
    "speaker": "^0.4.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.3",
    "@types/express": "^4.11.1",
    "@types/helmet": "0.0.37",
    "@types/jest": "^22.2.0",
    "@types/multer": "^1.3.6",
    "@types/node": "^9.4.2",
    "@types/socket.io": "^1.4.31",
    "jest": "^22.4.2",
    "nodemon": "^1.14.12",
    "ts-jest": "^22.4.1",
    "ts-node": "^4.1.0",
    "typescript": "^2.7.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": ".spec.ts$",
    "moduleFileExtensions": ["ts", "tsx", "js", "json", "node"],
    "collectCoverageFrom": ["src/**"]
  }
}
